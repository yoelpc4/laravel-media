{
    "name": "yoelpc4/laravel-media",
    "description": "Laravel Media manager.",
    "keywords": [
        "laravel",
        "media",
        "conversion",
        "image",
        "gd",
        "imagick",
        "document",
        "video",
        "audio"
    ],
    "homepage": "https://github.com/yoelpc4/laravel-media",
    "license": "MIT",
    "authors": [
        {
            "name": "Yoel Peter Chandra",
            "email": "yoelpc4@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-fileinfo": "*",
        "illuminate/contracts": "8.*|7.*|6.*",
        "illuminate/database": "8.*|7.*|6.*",
        "illuminate/http": "8.*|7.*|6.*",
        "illuminate/support": "8.*|7.*|6.*",
        "illuminate/validation": "8.*|7.*|6.*",
        "spatie/image": "^1.10.1",
        "spatie/temporary-directory": "^1.3.0"
    },
    "require-dev": {
        "orchestra/testbench": "6.*|5.*|4.*",
        "phpunit/phpunit": "9.*|8.4.*|8.3.*"
    },
    "autoload": {
        "psr-4": {
            "Yoelpc4\\LaravelMedia\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yoelpc4\\LaravelMedia\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yoelpc4\\LaravelMedia\\MediaServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
